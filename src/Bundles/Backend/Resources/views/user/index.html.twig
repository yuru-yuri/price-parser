{% extends 'backend.html.twig' %}

{% block title %}Users{% endblock %}
{% block body %}
    <ul>
        {% for user in users %}
        <li>
            <a href="{{ path('user_edit_backend',
                {id: user.id, login: user.login})
            }}">{{ user.login|capitalize }}</a>
        </li>
        {% endfor %}
    </ul>
{% endblock %}
